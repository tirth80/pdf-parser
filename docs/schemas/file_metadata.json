            "estimated_columns": {
              "type": "number"
            },
            "text_density": {
              "type": "number"
            },
            "char_count": {
              "type": "number"
            },
            "line_count": {
              "type": "number"
            },
            "char_density": {
              "type": "number"
            }
          },
          "required": [
            "page_number",
            "method",
            "quality_flag",
            "quality_score",
            "extraction_timestamp",
            "word_count",
            "layout_type",
            "estimated_columns",
            "text_density",
            "char_count",
            "line_count",
            "char_density"
          ]
